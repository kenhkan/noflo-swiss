{ "name": "noflo-swiss"
, "description": "Swiss Army Knife for NoFlo Programming"
, "keywords":
  [ "noflo"
  , "underscorejs"
  , "utility"
  ]
, "author": "Kenneth Kan <kenhkan@gmail.com>"
, "version": "0.0.5"
, "licenses":
  [ { "type": "MIT"
    , "url": "https://github.com/kenhkan/noflo-swiss/blob/master/LICENSE.md"
    }
  ]
, "engines":
  { "node": ">=0.6.0"
  }
, "repository":
  { "type": "git"
  , "url": "git://github.com/kenhkan/noflo-swiss"
  }
, "dependencies":
  { "noflo": "0.3.x"
  , "underscore": "1.4.x"
  , "underscore.string": "2.3.x"
  , "owl-deepcopy": "0.0.x"
  , "node-uuid": "1.4.x"
  , "noflo-packets": "0.0.x"
  }
, "devDependencies":
  { "coffeelint": "*"
  , "coffee-script": "1.4.x"
  , "noflo-test": "https://github.com/kenhkan/noflo-test/tarball/master"
  }
, "noflo":
  { "components":
    { "Compact": "./components/Compact.coffee"
    , "DeepCopy": "./components/DeepCopy.coffee"
    , "Quit": "./components/Quit.coffee"
    , "RandomUuid": "./components/RandomUuid.coffee"
    , "Zip": "./components/Zip.coffee"
    , "Trash": "./components/Trash.coffee"
    , "Hub": "./components/Hub.coffee"
    , "Underscore": "./components/Underscore.coffee"
    , "ErrorOnDisconnect": "./components/ErrorOnDisconnect.coffee"
    }
  }
, "scripts":
  { "pretest": "./node_modules/.bin/coffeelint -r components"
  , "test": "./node_modules/.bin/noflo-test --spec test/*.coffee"
  }
}
